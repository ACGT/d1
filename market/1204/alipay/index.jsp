						var resultCode = obj.resultCode;
						//resultCode < 0为抽兑奖失败,0这个返回码要注意,是"抽奖成功,但未抽中"
						if(resultCode <= 0){
							//显示对应的出错浮层
							showErrorMsg(resultCode);
						} else {
							//抽兑奖成功
							var prizeName = obj.prizeName;
							//根据奖品名称或者奖品id来判断中了啥
							if(prizeName == '联想笔记本电脑'){
								prizeID = 0;
